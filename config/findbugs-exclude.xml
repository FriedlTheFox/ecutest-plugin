<FindBugsFilter>
    <Match>
        <!-- Ignore warnings in Messages classes, generated by localizer. -->
        <Class name="~.*\.Messages" />
    </Match>
    <Match>
         <!-- Ignore warnings in Lexer and Parser classes, generated by ANTLR. -->
        <Package name="~.*\.filter" />
    </Match>
    <Match>
        <!-- Ignore warnings in unit test classes. -->
        <Class name="~.*Test$" />
        <Class name="~.*ST$" />
        <Class name="TestPluginManagerCleanUp" />
    </Match>
</FindBugsFilter>
